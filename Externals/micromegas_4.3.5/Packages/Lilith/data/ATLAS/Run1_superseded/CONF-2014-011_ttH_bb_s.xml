<!--
ATLAS result for mu(ttH, bb) at mH = 125. GeV
https://atlas.web.cern.ch/Atlas/GROUPS/PHYSICS/CONFNOTES/ATLAS-CONF-2014-011/
 -->

<expmu decay="bb" dim="1" type="n">
  <experiment>ATLAS</experiment>
  <source type="preliminary">CONF-2014-011</source>
  <sqrts>7+8</sqrts>
  <mass>125.</mass>

  <eff prod="ttH">1</eff>

  <bestfit>1.7</bestfit>
  
  <param>
    <uncertainty side="left">-1.4</uncertainty>
    <uncertainty side="right">1.4</uncertainty>
    <!-- if not needed, just <uncertainty></uncertainty> -->
  </param>
</expmu>
